program
    n, i, is_prime: int;
begin
    n := 29;
    i := 2;
    is_prime := 1;

    while (i < n) loop
        if ((n / i) = 0) then
            is_prime := 0;
        end if;
        
        i := i + 1;
    end loop;

    if (is_prime = 1) then
        output 1;
    else
        output 0;
    end if;
end;